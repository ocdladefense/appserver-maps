{
    "comment": "Module definition file.",
    "name": "map",
    "connectedApp":"default",
    "description": "Render Org data visually on maps.",
    "files": [
        "MapTemplate.php"
    ],
    "routes": {
        "maps": {
            "callback": "home",
            "content-type": "text\/html"
        },
        "maps/contacts": {
            "callback": "getMemberData",
            "content-type": "application\/json"
        },
        "maps/courts": {
            "callback": "getCourtData",
            "content-type": "application\/json"
        },
        "maps/witnesses": {
            "callback": "getWitnessData",
            "content-type": "application\/json"
        }

    }
}